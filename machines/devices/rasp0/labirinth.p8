--updates
function upd_normal()
	
	if btnp(➡️) then
	 player.x+=1
	 player.ox-=8
	 player.flipped=false
	 _upd=upd_anislide
	end
	if btnp(⬅️) then
	 player.x-=1
	 player.ox+=8
	 player.flipped=true
 	_upd=upd_anislide
	end
	if btnp(⬆️) then
	 player.y-=1
	 player.oy+=8
 	_upd=upd_anislide
	end
	if btnp(⬇️) then
	 player.y+=1
	 player.oy-=8
	 _upd=upd_anislide
	end
end

function upd_anislide()
 if player.ox>0 then
 	player.ox-=2
 end
 if player.ox<0 then
 	player.ox+=2
 end
 if player.oy>0 then
 	player.oy-=2
 end
 if player.oy<0 then
 	player.oy+=2
 end
 if player.ox==0 and player.oy==0 then
 	_upd=upd_normal
 end
end


function upd_gameover()

end
