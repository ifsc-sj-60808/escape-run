--draws
function draw_normal()
cls()
map()

--sets color 000000 
--to be not trasnparent 
palt(0, false)
spr(
 get_frame(player.ani),
 player.x*8+player.ox,
 player.y*8+player.oy,
 1,
 1,
 player.flipped
 )



end
