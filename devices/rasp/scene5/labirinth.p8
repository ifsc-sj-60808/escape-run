end

function move_mnu(wnd)
	if btnp(2) then
		wnd.cur=wnd.cur-1

		
		
	elseif btnp(3) then
		wnd.cur=wnd.cur+1
	end

 --cicles back	cursor--
	if wnd.cur==0 then 
		wnd.cur=#wnd.txt
	elseif wnd.cur==#wnd.txt+1 then
	 wnd.cur=1 
	end
	
	if btnp(5) and uitime<=0 then
		local tmp = true
		pcs[pc_on].val[wnd.cur] = not(pcs[pc_on].val[wnd.cur])
		
		uitime=100
		
		local cur = wnd.cur
		wnd.cur=wnd.cur-1
		
		del(wind, wnd)
		del(wind,statwind)
		sfx(11)
		
		showinv()
		invwind.cur=cur
		
	
		if pcs[pc_on].door!=verify_pcs()[pc_on] then
			if verify_pcs()[pc_on]==true then
				mset(pcs[pc_on].wirex,pcs[pc_on].wirey,12)
				sfx(5)
			else
				mset(pcs[pc_on].wirex,pcs[pc_on].wirey,3)
				sfx(5)
			end
		end
		pcs[pc_on].door = verify_pcs()[pc_on]
	end
end

