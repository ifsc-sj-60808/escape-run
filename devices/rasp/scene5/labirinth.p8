--gameplay

function moveplayer(dx,dy)
 local destx,desty=p_x+dx,p_y+dy
 local tile=mget(destx,desty)
 rock=getrock(destx,desty)
 pc_on=getpc(destx,desty)
 
 iswall=false
 
 if dx<0 then
  p_flipped=true
 elseif dx>0 then
  p_flipped=false   
 end
  
  
 if not iswalkable(destx,desty) then
  iswall=true
 end
 if rock then
  iswall=move_rock(rock, destx+dx,desty+dy,dx,dy)
 end
 
	if pc_on then
 	showinv()
 elseif iswall then 
  --wall
 	p_timer=0
	 
 	p_iox,p_ioy = 8*dx,8*dy
 	p_ox, p_oy = 0,0
 	
 	_upd=upd_pturn
 	_p_mov=mov_bump
 	if fget(tile,1) then
 		interact_tile(tile, destx, desty)
 	else
 		sfx(0)
 	end
 	
 else 
	 
	 
 	p_x+=dx
 	p_y+=dy
 	
	 p_timer=0
	 
 	p_iox,p_ioy = -8*dx,-8*dy
 	p_ox, p_oy = p_iox,p_ioy
 	
 	_upd=upd_pturn
 	_p_mov=mov_walk
 	sfx(2)
 end
 check_btn_pzls()
	interact_obj("key", collect_object)
	interact_obj("stairs", do_stair)
 rtel()
end

function move_rock(rock,r_destx,r_desty,dx,dy)

 tile=mget(r_destx,r_desty)
 
 next_rock=getrock(r_destx,r_desty)
 
 if next_rock then
 	move_rock(next_rock,r_destx+dx,r_desty+dy,dx,dy)	
 end
 
	if fget(tile,3) then
	
		rock.x=r_destx
 	rock.y=r_desty
 	
 	rock.iox,rock.ioy= -8*dx,-8*dy
 	rock.ox,rock.oy = -8*dx,-8*dy
		rock.timer=0 	
 	return false
	else
		return true
	end
end

function interact_tile(tile, destx, desty)

		if tile == 13 then
			--doors
			if p_keys>0 then
				mset(destx, desty,12)
				p_keys-=1
 			sfx(5)	
			else
				sfx(0)
			end
 	elseif tile == 11 then
 		--computer
 		--make popup
			showinv()
		
 	elseif tile == 8 or tile == 9 then
 	 --stones
 	 sfx(3)
 	elseif tile == 3 then
 	 sfx(0)

 	elseif tile==80 then
 		_drw=draw_gover
 		_upd=upd_gover
		end
end

function interact_obj(_typ, func)
	for obj in all(objects) do
		if obj.typ==_typ then
			if obj.x==p_x and obj.y==p_y then
				func(obj)
				return
			end
		end
	end
end

function rtel()
local pos = false	
	for obj in all(objects) do
		if obj.typ=="rdel" then		
			if obj.wirex == p_x and obj.wirey==p_y then
				pos=true
			end
		
			for rock in all(objects) do
				if rock.typ=="rock" and rock.x==obj.x and not
			 getrock(obj.wirex,obj.wirey)
    and rock.y==obj.y and not pos then
				 rock.x=obj.wirex
				 rock.y=obj.wirey
				 sfx(6)
			 end
		 end
	 end
 end
end

function do_stair(obj)
	p_x=obj.wirex
	p_y=obj.wirey
	p_iox=0
	p_ioy=0
	p_ox=0
	p_oy=0
	return
end

function collect_object(obj)
	--collectables
	p_keys+=1
	sfx(7)
	del(objects,obj)	

end

function unfog()
	for x=0,15 do
		for y=0,15 do
			if los(p_x,p_y,x+16*p_room,y+16*p_floor) then
    unfogtile(x,y)
			else
			 fog[p_room][p_floor][x][y]=1
			end
		end
	end
end 


function unfogtile(x,y)
 fog[p_room][p_floor][x][y]=0
 if iswalkable(x,y,"sight") then
  for i=1,4 do
   local tx,ty=x+dirx[i],y+diry[i]
   if inbounds(tx,ty) and not iswalkable(tx,ty,"sight") then
    fog[p_room][p_floor][x][y]=0
   end
  end  
 end
end


function los(x1,y1,x2,y2)
 local frst,sx,sy,dx,dy=true
 --â˜…
 if dist(x1,y1,x2,y2)==1 then return true end
 if x1<x2 then
  sx=1
  dx=x2-x1
 else
  sx=-1
  dx=x1-x2
 end
 if y1<y2 then
  sy=1
  dy=y2-y1
 else
  sy=-1
  dy=y1-y2
 end
 
 if dx>3 then return false end
 if dy>3 then return false end
 
 local err, e2 = dx-dy, nil
 
 while not(x1==x2 and y1==y2) do
  if not frst and iswalkable(x1,y1,"sight")==false then return false end
  frst=false
  e2=err+err
  if e2>-dy then
   err=err-dy
   x1=x1+sx
  end
  if e2<dx then
   err=err+dx
   y1=y1+sy
  end
 end
 return true 
end

function update_room()
	
		
	if p_x <16 then
		p_room=0
	elseif p_x < 32 then
		p_room=1
	elseif p_x < 48 then
	 p_room=2
	end
 
 if p_y <16 then
		p_floor=0
	elseif p_y < 32 then
		p_floor=1
	end 
end

function check_btn_pzls()
	
	local active_buffer=false

	for bttn in all(objects) do
	
		active_buffer=bttn.active
		local tmp= false

		if bttn.typ=="button" then
			for r in all(objects) do
				if r.typ=="rock"then
					if bttn.x ==r.x and bttn.y==r.y then
						bttn.active=true
						bttn.done=false
						do_btn_pzls(bttn, active_buffer)
						tmp=true
					end
				end
			end
			if bttn.x==p_x and bttn.y==p_y then
				bttn.active=true
				tmp=true
				bttn.done=false
			 do_btn_pzls(bttn, active_buffer)
				return
			end
		end
		if tmp==false then
			bttn.active=false
			do_btn_pzls(bttn, active_buffer)
		end
	end
end

function do_btn_pzls(bttn,buffer)
	
	if bttn.active == buffer 		
	then return end
					
	if bttn.active==true then
		sfx(5)

	 mset(bttn.wirex, bttn.wirey,12)
	else 	
		sfx(5)

		mset(bttn.wirex, bttn.wirey,3)
 
	end
end

