function upd_gstart()
	if talkwind != nil then
		if btnp(5) then
			_upd = upd_normal
			_drw = draw_normal
			wind={}
		end
	end
end

function upd_normal()
 buffer_btn()
 dobtn(btn_memory)
 btn_memory=-1
 update_room()
 check_btn_pzls()
end

function upd_pturn()
	buffer_btn()
 p_timer=min(p_timer+0.3,1)
 for r in all(objects) do
	 r.timer=min(r.timer+0.3,1)
 end
 
 _p_mov()
 unfog()
 if p_timer==1 then
  _upd=upd_normal
 end
end

function mov_walk()
 p_ox=p_iox*(1-p_timer)
 p_oy=p_ioy*(1-p_timer)
	
	for r in all(objects) do
		r.ox=r.iox*(1-r.timer)
	 r.oy=r.ioy*(1-r.timer)
	end
end

function mov_bump()
 
 if p_timer<0.5 then
 	p_ox=p_iox*p_timer
 	p_oy=p_ioy*p_timer
 else
 	p_ox=p_iox*(1-p_timer)
 	p_oy=p_ioy*(1-p_timer)
 end
end

