--tools
function get_frame(ani)
	return ani[flr(t/15)%4 + 1]

end


function rectfill2(_x,_y,_w,_h,_c)
 rectfill(_x,_y,_x+_w-1,_y+_h-1,_c)
end

function oprint8(_t,_x,_y,_c,_c2)
 for i=1,8 do
  print(_t,_x+dirx[i],_y+diry[i],_c2)
 end 
 print(_t,_x,_y,_c)
end


function blankmap(_default)
	local ret={}
	if(_default==nil) _default=1
 for room=0,2 do
 	ret[room]={}
 	for floor=0,1 do 
 		ret[room][floor]={}
			for x=0,15 do
				ret[room][floor][x]={}
				for y=0,15 do
					ret[room][floor][x][y]=_default
				end
			end
		end
	end
	return ret
end

function dist(fx,fy,tx,ty)
 local dx,dy=fx-tx,fy-ty
 return sqrt(dx*dx+dy*dy)
end

function getrock(x,y)
 for r in all(objects) do
  if r.typ=="rock" and r.x==x and r.y==y then
   return r
  end
 end
 return false
end

function getpc(x,y)
 for pc in all(pcs) do
  if pc.x==x and pc.y==y then
   return pc.num
  end
 end
 return false
end

function iswalkable(x,y,mode)
 if mode== nil then mode="" end
 --sight
 if inbounds(x,y) then
  local tle=mget(x,y)
  if mode=="sight" then
   return not fget(tle,2)
  else
   if fget(tle,0)==false then
    return true
   end
  end
 end
 return false
end

function inbounds(x,y)
 return not (x<0 or y<0 or x>63 or y>63)
end


function addobject(r_id,rx,ry,r_typ,r_wirex,r_wirey)
 local r={
  typ=r_typ,
  id=r_id,
  x=rx,
  y=ry,
  ox=0,
  oy=0,
  iox=0,
  ioy=0,
  timer=1,
  flash=0,
  active=false,
  done=true,
  wirex=r_wirex,
  wirey=r_wirey
  }
 add(objects,r)
 return r
end


function verify_pcs()
	
 local	returnval={}
	for pc=1, #pcs do
	
	 local pcval=true

		for i = 1, 10 do
			if pcs[pc].val[i] != pcs[pc].answer[i] then
				pcval=false
			end
		end
		add(returnval, pcval)
			
		
	end
	return returnval
end
