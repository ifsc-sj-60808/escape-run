--gpio

--runs once, at _init
function declare_bools()

 pin1=0
 pin2=0
 pin3=0
 
 -- 2 bytes da memoria
	local buf_out=0x4300
	local buf_in =0x4301

end

--runs at update
function update_bools()
	
	pin1=1
	-- so pra testar ai jeremias
	
	write_flags_gpio(pin1,pin2,pin3)
	send_flags_bitwise(pin1,pin2,pin3)

end

function gpio(pin, val)
	serial(pin, val and one or zero, 1)
end

function write_flags_gpio(a,b,c)
  poke(0x5f80+0, a and 255 or 0) -- wpi 0 (bcm17) 
  poke(0x5f80+1, b and 255 or 0) -- wpi 1 (bcm18)
  poke(0x5f80+2, c and 255 or 0) -- wpi 2 (bcm27)
end

  -- bor sets current bit of 
  -- (v, ) to ( ,this)
  -- cursor starts at 0000000x
  -- shl moves cursor to left 
  -- ( ,n) times and is (x, )

function send_flags_bitwise(a,b,c)
  
  local v=0 
  -- value between 0 and 255
  
  if a then 
   v=bor(v,1) 
  end
  -- sets 00000000 to 00000001 
  
  if b then 
   v=bor(v,shl(1,1)) 
  end
  -- sets 00000000 to 00000010
  
  if c then 
   v=bor(v,shl(1,2))
  end
  -- sets 00000000 to 00000100
  
  poke(buf_out,v)
  serial(0x807, buf_out, 1)        -- escreve no -o
end

function send_flags_arithmetic(a,b,c)
 local v = a + b*2 + c*4
 poke(buf_out,v)
 serial(0x807, buf_out, 1)        -- escreve no -o
end


function poll_host()
  local n=serial(0x806, buf_in, 1) -- lれち do -i
  if n>0 then 
   return peek(buf_in) 
  end
  return nil
end

