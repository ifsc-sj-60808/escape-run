--gameplay

function moveplayer(dx,dy)
 local destx,desty=p_x+dx,p_y+dy
 local tile=mget(destx,desty)
 
 if dx<0 then
  p_flipped=true
 elseif dx>0 then
  p_flipped=false   
 end
  
 if not iswalkable(destx,desty) then
  --wall
 	p_timer=0
	 
 	p_iox,p_ioy = 8*dx,8*dy
 	p_ox, p_oy = 0,0
 	
 	_upd=upd_pturn
 	_p_mov=mov_bump
 	if fget(tile,1) then
 		interact(tile, destx, desty)
 	else
 		sfx(0)
 	end
 	
 else 
 	p_x+=dx
 	p_y+=dy
 	
	 p_timer=0
	 
 	p_iox,p_ioy = -8*dx,-8*dy
 	p_ox, p_oy = p_iox,p_ioy
 	
 	_upd=upd_pturn
 	_p_mov=mov_walk
 	sfx(2)
 end
end

function interact(tile, destx, desty)

		if tile == 13 then
			--doors
 		mset(destx, desty,12)
 		sfx(1)
 	elseif tile == 11 then
 		--computer
 		--make popup
 	elseif tile == 8 or tile == 9 then
 	 --stones
 	 sfx(3)

 	end
end

function unfog()
	for x=0,15 do
		for y=0,15 do
			if los(p_x,p_y,x,y) then
				fog[p_room][x][y]=0
			else
			 fog[p_room][x][y]=1
			end
		end
	end
end 
function los(x1,y1,x2,y2)
 local frst,sx,sy,dx,dy=true
 --â˜…
 if dist(x1,y1,x2,y2)==1 then return true end
 if x1<x2 then
  sx=1
  dx=x2-x1
 else
  sx=-1
  dx=x1-x2
 end
 if y1<y2 then
  sy=1
  dy=y2-y1
 else
  sy=-1
  dy=y1-y2
 end
 
 if dx>2 then return false end
 if dy>2 then return false end
 
 local err, e2 = dx-dy, nil
 
 while not(x1==x2 and y1==y2) do
  if not frst and iswalkable(x1,y1,"sight")==false then return false end
  frst=false
  e2=err+err
  if e2>-dy then
   err=err-dy
   x1=x1+sx
  end
  if e2<dx then
   err=err+dx
   y1=y1+sy
  end
 end
 return true 
end
