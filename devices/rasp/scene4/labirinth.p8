--updates
function upd_normal()
 buffer_btn()
 dobtn(btn_memory)
 btn_memory=-1
end

function upd_pturn()
	buffer_btn()
 p_timer=min(p_timer+0.3,1)
 
 _p_mov()
 unfog()
 if p_timer==1 then
  _upd=upd_normal
 end
end


function upd_gameover()

end

function mov_walk()
 p_ox=p_iox*(1-p_timer)
 p_oy=p_ioy*(1-p_timer)
end

function mov_bump()
 
 if p_timer<0.5 then
 	p_ox=p_iox*p_timer
 	p_oy=p_ioy*p_timer
 else
 	p_ox=p_iox*(1-p_timer)
 	p_oy=p_ioy*(1-p_timer)
 end
end

function buffer_btn()
	if btn_memory==-1 then
		btn_memory = getbtn()
 end
end

function getbtn()
 for i=0,5 do
  if btnp(i) then
   return i
  end
 end
 return -1
end

function dobtn(btn_mem)
 if btn_mem<0 then return end
 if btn_mem<4 then
  moveplayer(dirx[btn_mem+1],diry[btn_mem+1])
  return
 end
 -- menu button
	if btn_mem==4 then
		gpio(1, 1)
	end
	if btn_mem==5 then
		showinv()
	end
end



