--tools
function get_frame(ani)
	return ani[flr(t/15)%4 + 1]

end


function rectfill2(_x,_y,_w,_h,_c)
 rectfill(_x,_y,_x+_w-1,_y+_h-1,_c)
end



function blankmap(_default)
	local ret={}
	if(_default==nil) _default=1
 for lvl=0,5 do
 	ret[lvl]={}
		for x=0,15 do
			ret[lvl][x]={}
			for y=0,15 do
				ret[lvl][x][y]=_default
			end
		end
	end
	return ret
end

function dist(fx,fy,tx,ty)
 local dx,dy=fx-tx,fy-ty
 return sqrt(dx*dx+dy*dy)
end

function iswalkable(x,y)
 --sight
 local tle=mget(x,y)
 if fget(tle,0)==false then
  return true
 end
 return false
end
